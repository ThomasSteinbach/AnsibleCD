<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl plugin="mailer@1.11">
  <hudsonUrl>{{ ACI_SERVER_URL }}</hudsonUrl>
  <smtpAuthUsername>{{ aci_smtp_auth_user }}</smtpAuthUsername>
  <smtpAuthPassword>{{ aci_smtp_auth_password }}</smtpAuthPassword>
  <replyToAddress>{{ aci_smtp_reply_to }}</replyToAddress>
  <smtpHost>{{ aci_smtp_host }}</smtpHost>
  <useSsl>false</useSsl>
  <smtpPort>{{ aci_smtp_port }}</smtpPort>
  <charset>UTF-8</charset>
</hudson.tasks.Mailer_-DescriptorImpl>
